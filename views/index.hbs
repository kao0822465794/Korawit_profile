
<body style="background-color: grey; margin-top:5%;">
    <center>
        <h1 class="title">MYPROFILE</h1>
    </center>
   <h1 class="name">My name is {{myData.name}}</h1>
    <center>
    <div class="img" align="center">
        <img src="img.jpg" alt="">
    </div>
</center>

<div class="container">
<h1>Personal information</h1>
    {{#with myData.aboutme}}
<h3>Frist - Last Name :{{FullName}} </h3>
<h3>Tel :{{Phone}} </h3>
<h3>Email :{{Email}} </h3>
<h3>Address:{{Address}} </h3>
</div>        
{{/with}}


    <div class="container" style="margin-top: 3%;">
        <h3>Educational history</h3>
        {{#with myData.education}}
        <p ><b>Upper Education : </b>{{Upper}}</p>
        <p ><b>Lower Education : </b>{{Lower}}</p>
        {{/with}}</div>

    <div class="container" style="margin-top: 5%;">
        <h1 align="center">Skill</h1>
     <div class="textskill">
    {{#with myData.skill}}
